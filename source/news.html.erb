---
title: News &middot; Oswald Labs
---

<header id="masthead" class="light">
	<div class="container pt-3 pb-3">
		<%= partial "navbar" %>
	</div>
	<%= partial "presskit_nav" %>
</header>

<main id="content">
	<section class="pt-5">
		<div class="container">
			<div class="text-center">
				<div class="mb-5">
					<h1>News</h1>
				</div>
			</div>
			<div class="row justify-content-center">
				<div class="col-md-8">
					<div>
						<% blog.articles[0...5].each do |article| %>
							<article class="mb-5">
								<h2 class="h4">
									<a href="<%= article.url.chomp("index.html").chomp(".html") %>"><%= article.title %></a>
								</h2>
								<p class="text-secondary"><time><%= article.date.strftime('%B %e, %Y') %></time></p>
								<%= article.summary %>
								<a href="<%= article.url.chomp("index.html").chomp(".html") %>">Continue reading &rarr;</a>
							</article>
						<% end %>
					</div>
				</div>
			</div>
		</div>
	</section>
</main>